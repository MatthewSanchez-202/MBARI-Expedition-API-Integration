            "description": "",
            "example": "2024-03-22 12:00:00"
          },
          "ScheduledEndDtg": {
            "type": "string",
            "format": "datetime",
            "description": "",
            "example": "2024-03-23 12:00:00"
          },
          "EquipmentDesc": {
            "type": "string",
            "format": "varchar(8000)",
            "description": "",
            "example": "EquipmentDesc"
          },
          "Participants": {
            "type": "string",
            "format": "varchar(2048)",
            "description": "",
            "example": "Fabian Santano, Marc Garcia, Matthew Sanchez, Nikhil Kulkarni, Ryan Romero"
          },
          "RegionDesc": {
            "type": "string",
            "format": "varchar(2048)",
            "description": "",
            "example": "RegionDesc"
          },
          "PlannedTrackDesc": {
            "type": "string",
            "format": "varchar(6144)",
            "description": "",
            "example": "PlannedTrackDesc"
          },
          "StartDtg": {
            "type": "string",
            "format": "datetime",
            "description": "",
            "example": "2024-04-22 12:00:00"
          },
          "EndDtg": {
            "type": "string",
            "format": "datetime",
            "description": "",
            "example": "2024-04-23 12:00:00"
          },
          "Accomplishments": {
            "type": "string",
            "format": "varchar(6144)",
            "description": "",
            "example": "CSUMB Graduate"
          },
          "ScientistComments": {
            "type": "string",
            "format": "varchar(6144)",
            "description": "",
            "example": "Fabian Santano, Marc Garcia, Matthew Sanchez, Nikhil Kulkarni, Ryan Romero"
          },
          "SciObjectivesMet": {
            "type": "string",
            "format": "varchar(50)",
            "description": "",
            "example": "SciObjectivesMet"
          },
          "OperatorComments": {
            "type": "string",
            "format": "varchar(2048)",
            "description": "",
            "example": "Expedition went as Planned"
          },
          "AllEquipmentFunctioned": {
            "type": "string",
            "format": "varchar(50)",
            "description": "",
            "example": "Yes All equipment"
          },
          "OtherComments": {
            "type": "string",
            "format": "varchar(2048)",
            "description": "",
            "example": "Swagger Is Being updated even more as i type this."
          },
          "UpdatedBy": {
            "type": "string",
            "format": "varchar(50)",
            "description": "",
            "example": "Team 7 Member : Fabian Santano"
          },
          "ismodified": {
            "type": "integer",
            "format": "int",
            "description": "Int should be 0 or 1",
            "example": 0
          }
        },
        "xml": {
          "name": "expedition"
        }
      },
      "Dive": {
        "type": "object",
        "properties": {
          "DiveID": {
            "type": "integer",
            "format": "int not null",
            "example": 1
          },
          "DeviceID": {
            "type": "integer",
            "format": "int",
            "example": 1
          },
          "RovName": {
            "type": "string",
            "format": "varchar(5)",
            "example": "ROV1"
          },
          "DiveNumber": {
            "type": "integer",
            "format": "int",
            "example": 101
          },
          "ExpeditionID_FK": {
            "type": "integer",
            "format": "int",
            "example": 1001
          },
          "DiveStartDtg": {
            "type": "string",
            "format": "datetime",
            "example": "2024-03-06 08:00:00"
          },
          "DiveEndDtg": {
            "type": "string",
            "format": "datetime",
            "example": "2024-03-06 11:30:00"
          },
          "DiveChiefScientist": {
            "type": "string",
            "format": "varchar(50)",
            "example": "Dr. Smith"
          },
          "BriefAccomplishments": {
            "type": "string",
            "format": "varchar(5000)",
            "example": "Successful exploration of underwater caves."
          },
          "DiveStartTimecode": {
            "type": "string",
            "format": "char(11)",
            "example": "00:00:00:01"
          },
          "DiveEndTimecode": {
            "type": "string",
            "format": "char(11)",
            "example": "00:00:03:01"
          },
          "DiveLatMid": {
            "type": "number",
            "format": "decimal(12, 8)",
            "example": 34.56789
          },
          "DiveLonMid": {
            "type": "number",
            "format": "decimal(14, 8)",
            "example": -123.45678
          },
          "DiveDepthMid": {
            "type": "number",
            "format": "decimal(7, 1)",
            "example": 45.6
          }
        },
        "xml": {
          "name": "dive"
        }
      }
    }
  }
