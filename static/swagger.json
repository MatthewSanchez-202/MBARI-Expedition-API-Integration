  "components": {
    "schemas": {
      "Expedition": {
        "type": "object",
        "properties": {
          "ExpeditionID": {
            "type": "integer",
            "format": "int",
            "example": 1001
          },
          "DeviceID": {
            "type": "integer",
            "format": "int",
            "example": 1
          },
          "ShipName": {
            "type": "string",
            "format": "varchar(5)",
            "example": "Ship1"
          },
          "ShipSeqNum": {
            "type": "integer",
            "format": "int",
            "example": 1
          },
          "Purpose": {
            "type": "string",
            "format": "varchar(8000)",
            "description": "",
            "example": "Purpose is to test this API "
          },
          "StatCode": {
            "type": "string",
            "format": "varchar(30)",
            "description": "",
            "example": "TestStatCode: 3000"
          },
          "ExpdChiefScientist": {
            "type": "string",
            "format": "varchar(50)",
            "description": "",
            "example": "Mike McCann"
          },
          "ExpdPrincipalInvestigator": {
            "type": "string",
            "format": "varchar(50)",
            "description": "",
            "example": "Fabian Santano"
          },
          "ScheduledStartDtg": {
            "type": "string",
