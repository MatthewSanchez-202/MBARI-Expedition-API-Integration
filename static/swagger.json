{
  "swagger": "2.0",
  "info": {
    "description": "API for managing expeditions, dives, and related data.",
    "version": "1.0.0",
    "title": "Expedition and Dive API"
  },
  "host": "localhost:5000",
  "basePath": "/",
  "tags": [
    {
      "name": "Expeditions",
      "description": "API for managing expeditions"
    },
    {
      "name": "Dives",
      "description": "API for managing dives"
    },
    {
      "name": "Calendar",
      "description": "API for managing calendar entries"
    },
    {
      "name": "Camlog",
      "description": "API for managing camera log data"
    }
  ],
  "schemes": [
    "http"
  ],
  "paths": {
    "/get-all-expeditions": {
      "get": {
        "tags": ["Expeditions"],
        "summary": "Retrieve all expeditions",
        "description": "Returns a list of all expeditions with options to filter by ship name, date range, and sort.",
        "parameters": [
          {
            "name": "sortfield",
            "in": "query",
            "description": "Field to sort the expeditions by",
            "required": false,
            "type": "string",
            "default": "ExpeditionID"
          },
          {
            "name": "sortorder",
            "in": "query",
            "description": "Order to sort the expeditions (ASC or DESC)",
            "required": false,
            "type": "string",
            "default": "ASC"
          },
          {
            "name": "shipname",
            "in": "query",
            "description": "Filter by the ship name involved in the expedition",
            "required": false,
            "type": "string"
          },
          {
            "name": "startdate",
            "in": "query",
            "description": "Filter by the start date of the expedition (YYYY-MM-DD)",
            "required": false,
            "type": "string"
          },
          {
            "name": "enddate",
            "in": "query",
            "description": "Filter by the end date of the expedition (YYYY-MM-DD)",
            "required": false,
            "type": "string"
          },
          {
            "name": "year",
            "in": "query",
            "description": "Filter by year of the expedition",
            "required": false,
            "type": "integer"
          }
        ],
        "responses": {
          "200": {
            "description": "A list of expeditions",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Expedition"
              }
            }
          },
          "400": {
            "description": "Invalid input, object invalid"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/get-all-dives": {
      "get": {
        "tags": ["Dives"],
        "summary": "List all dives",
        "description": "Retrieve a list of all dives with optional sorting.",
        "parameters": [
          {
            "name": "sortfield",
            "in": "query",
            "required": false,
            "type": "string",
            "default": "DiveID",
            "description": "Field to sort the dives by."
          },
          {
            "name": "sortorder",
            "in": "query",
            "required": false,
            "type": "string",
            "default": "ASC",
            "description": "Order to sort the dives (ASC or DESC)."
          },
          {
            "name": "rovname",
            "in": "query",
            "description": "Filter by the ROV name involved in the dive",
            "required": false,
            "type": "string"
          },
          {
            "name": "startdate",
            "in": "query",
            "description": "Filter by the start date of the dive (YYYY-MM-DD)",
            "required": false,
            "type": "string"
          },
          {
            "name": "enddate",
            "in": "query",
            "description": "Filter by the end date of the dive (YYYY-MM-DD)",
            "required": false,
            "type": "string"
          },
          {
            "name": "year",
            "in": "query",
            "description": "Filter by year of the dive",
            "required": false,
            "type": "integer"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Dive"
              }
            }
          },
          "400": {
            "description": "Invalid sort field or order"
          },
          "500": {
            "description": "Server Error"
          }
        }
      }
    },
    "/get-all-calendar": {
      "get": {
        "tags": ["Calendar"],
        "summary": "List all calendar entries",
        "description": "Retrieve a list of all calendar entries for expeditions and dives.",
        "responses": {
          "200": {
            "description": "Successful operation",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/CalendarEntry"
              }
            }
          },
          "500": {
            "description": "Server Error"
          }
        }
      }
    },
    "/get_all_camlogdata": {
      "get": {
        "tags": ["Camlog"],
        "summary": "List all camera log entries",
        "description": "Retrieve a list of all camera log entries related to dives.",
        "responses": {
          "200": {
            "description": "Successful operation",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/CamlogEntry"
              }
            }
          },
          "404": {
            "description": "Not Found. The requested URL was not found on the server."
          },
          "500": {
            "description": "Server Error"
          }
        }
      }
    },
    "/get_all_DocRickettsPilotsDiveStaging": {
      "get": {
        "tags": ["DocRicketts"],
        "summary": "Retrieve data from DocRicketts Pilots Dive Staging",
        "description": "Fetches data from the DocRicketts Pilots Dive Staging table.",
        "responses": {
          "200": {
            "description": "Successful operation",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/DocRickettsPilotsDive"
              }
            }
          }
        }
          }

  },
  "500": {
    "description": "Server error or database connection issue",
    "schema": {
      "type": "object",
      "properties": {
        "error": {
          "type": "string"
        }
      }
    }
  },
"/adminerror": {
"get": {
"tags": ["Admin"],
"summary": "Retrieve administrative errors",
"description": "Fetches data related to administrative errors from the Bad Still Image URL table.",
"responses": {
  "200": {
    "description": "Successful operation",
    "schema": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/AdminErrors"
      }
    }
  },
  "500": {
    "description": "Server error or database connection issue",
    "schema": {
      "type": "object",
      "properties": {
        "error": {
          "type": "string"
        }
      }
    }
  }
}
}
}
},

"definitions": {
    "Expedition": {
      "type": "object",
      "properties": {
        "ExpeditionID": {
          "type": "integer",
          "format": "int64"
        },
        "ShipName": {
          "type": "string"
        },
        "StartDate": {
          "type": "string",
          "format": "date"
        },
        "EndDate": {
          "type": "string",
          "format": "date"
        }
      }
    },
    "Dive": {
      "type": "object",
      "properties": {
        "DiveID": {
          "type": "integer",
          "format": "int64"
        },
        "RovName": {
          "type": "string"
        },
        "StartDate": {
          "type": "string",
          "format": "date"
        },
        "EndDate": {
          "type": "string",
          "format": "date"
        }
      }
    },
    "CalendarEntry": {
      "type": "object",
      "properties": {
        "Date": {
          "type": "string",
          "format": "date"
        },
        "Event": {
          "type": "string"
        }
      }
    },
    "CamlogEntry": {
      "type": "object",
      "properties": {
        "Timestamp": {
          "type": "string",
          "format": "date-time"
        },
        "Event": {
          "type": "string"
        },
        "Camera": {
          "type": "string"
        }
      }
    },
    "DocRickettsPilotsDive": {
      "type": "object",
      "properties": {
        "Column1": {
          "type": "string"
        },
        "Column2": {
          "type": "integer"
        }
  }
},
"DocRickettsPilotsDiveStaging": {
  "type": "object",
  "properties": {
    "Column1": {"type": "string"},
    "Column2": {"type": "integer"}
  }

  },

   "AdminErrors": {
    "type": "object",
    "properties": {
      "Column1": {"type": "string"},
      "Column2": {"type": "integer"}
    }
}
  }
}
