            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Expedition"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful insertion of new MBARI expedition into Expedition table"
          },
          "400": {
            "description": "Invalid Expedition ID"
          },
          "500": {
            "description": "error"
          }
        }
      }
    }
    ,
